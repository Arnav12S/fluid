    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/react": "2.6.0",
    "@sanity/client": "^6.12.3",
    "@sanity/image-url": "^1.0.2",
    "@sanity/overlays": "^2.3.9",
    "@sanity/react-loader": "^1.8.2",
    "@shopify/cli": "3.53.0",
    "@shopify/cli-hydrogen": "^7.0.0",
    "@shopify/hydrogen": "^2024.1.0",
    "@shopify/hydrogen-react": "^2024.1.0",
    "@shopify/remix-oxygen": "^2.0.3",
    "@vercel/stega": "^0.1.0",
    "class-variance-authority": "^0.7.0",
    "color2k": "^2.0.3",
    "cva": "1.0.0-beta.1",
    "embla-carousel": "8.0.0-rc22",
    "embla-carousel-autoplay": "8.0.0-rc22",
    "embla-carousel-react": "8.0.0-rc22",
    "framer-motion": "^11.0.3",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "groq": "^3.27.1",
    "groqd": "^0.15.10",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
